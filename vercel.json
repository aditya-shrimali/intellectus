{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.{js,ts,jsx,tsx}",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ],
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src"
  }
  